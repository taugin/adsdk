<resources>
    <declare-styleable name="ScBlinkView">
        <attr name="startAlpha" format="integer" />
        <attr name="endAlpha" format="integer" />
        <attr name="baseColor" format="color" />
        <attr name="blinkDuration" format="integer" />
    </declare-styleable>
    <declare-styleable name="ScDView">
        <attr name="stepCount" format="integer" />
        <attr name="emptyAlpha" format="integer" />
        <attr name="dotBaseColor" format="color" />
        <attr name="filledAlpha" format="integer" />
        <attr name="dotRadius" format="dimension" />
    </declare-styleable>
</resources>
